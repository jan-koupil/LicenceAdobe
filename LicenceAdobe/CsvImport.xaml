<Window x:Class="LicenceAdobe.CsvImport"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LicenceAdobe"
        mc:Ignorable="d"
        Title="Import CSV" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="csvData" Margin="10,10,9.6,34.4" TextWrapping="Wrap" AcceptsReturn="True" ToolTip="Vložte CSV z webu Adobe"/>
        <Button x:Name="closeCsvBtn" Content="Importovat" Margin="0,0,9.6,10.4" Click="closeCsvBtn_Click" Height="19" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75"/>
    </Grid>
</Window>
